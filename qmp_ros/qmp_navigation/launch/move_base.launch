<launch>

  <master auto="start"/>
  <!-- Arguments -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="odom" />
  <arg name="move_forward_only" default="false"/>

 <!-- Already requested on the qmp_navigation.launch!! -->
 <!-- Run the map server --> 
    <!-- <node name="map_server" pkg="map_server" type="map_server" args="$(find qmp)/qmp_ros/qmp_navigation/maps/qmp_world.yaml"/> -->

 <!--- Run AMCL --> 
    <!-- <include file="$(find amcl)/examples/amcl_diff.launch" /> -->
    
 <!-- move_base -->
   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
   <param name="base_global_planner" value="RAstar_planner/RAstarPlannerROS"/> 
    <rosparam file="$(find qmp)/qmp_ros/qmo_navigation/param/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find qmp)/qmp_ros/qmo_navigation/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find qmp)/qmp_ros/qmo_navigation/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find qmp)/qmp_ros/qmo_navigation/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find qmp)/qmp_ros/qmp_navigation/param/move_base_params.yaml" command="load" />
    <rosparam file="$(find qmp)/qmp_ros/qmo_navigation/param/mpc_local_planner_params.yaml" command="load" />
    
    <remap from="cmd_vel" to="$(arg cmd_vel_topic)"/>
    <remap from="odom" to="$(arg odom_topic)"/>
 </node>

</launch> 
